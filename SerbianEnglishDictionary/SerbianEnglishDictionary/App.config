<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
	<appSettings>
		<!--Possible dictionary names are "SentencesDictionary.csv" "PhrasesDictionary.csv" and "WordsDictionary.csv"-->
		<add key="DictionaryPath" value="../../Source/WordsDictionary.csv"/>
		<!--Possible values are "EnglishToSerbian" and "SerbianToEnglish"-->
		<add key="TranslationWay" value="SerbianToEnglish"/>
		<!--Possible values are "All" and "LastAdded"-->
		<add key="EntitiesFilterType" value="LastAdded"/>
		<!--EntitiesCountPercentage value can be from 0 to 100. It is not considered when the value of the EntitiesFilterType is "All"-->
		<add key="EntitiesCountPercentage" value="30"/>
	</appSettings>
</configuration>