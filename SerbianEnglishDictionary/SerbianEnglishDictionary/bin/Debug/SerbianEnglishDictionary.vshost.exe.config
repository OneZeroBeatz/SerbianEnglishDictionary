<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
	<appSettings>
		<!--Possible dictionar names are "PhrasesDictionary.csv" and "WordsDictionary.csv"-->
		<add key="DictionaryPath" value="../../Source/PhrasesDictionary.csv"/>
		<!--Possible values are "EnglishToSerbian" and "SerbianToEnglish"-->
		<add key="TranslationWay" value="EnglishToSerbian"/>
		<!--Possible values are "All" and "LastAdded"-->
		<add key="EntitiesFilterType" value="LastAdded"/>
		<!--EntitiesCountLimit is not considered when the value of the EntitiesFilterType is "All" "-->
		<add key="EntitiesCountLimit" value="1"/>
	
	</appSettings>
</configuration>